@model List<Initiative.Models.ValoresModel>

<h3 style="margin-top: 2%;">Valores Atualizados</h3><br/>

@foreach (var item in Model)
{
    <div>
        <dl class="row">
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(Modelitem => item.Valores)
            </dt>
            <dd class = "col-sm-10">
                R$ @Html.DisplayFor(Modelitem => item.Valores)
            </dd>

            <dt class = "col-sm-2">
                @Html.DisplayNameFor(Modelitem => item.Decomposto)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(Modelitem => item.Decomposto)
            </dd>
        </dl>
    </div>

    <hr/>
}